<?xml version="1.0" encoding="UTF-8"?>
<match-fields xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="matching.xsd"
	agreement-weight-threshold="1.0" match-weight-threshold="0.8">

	<!-- 业务匹配默认配置 -->

	<match-field field-name="BIZ_INPATIENTNO"
		agreement-probability="0.9" disagreement-probability="0.2"
		comparator-function="id" match-threshold="0.8" weight="1" desc="住院号" />

	<match-field field-name="BIZ_CLINICNO"
		agreement-probability="0.9" disagreement-probability="0.2"
		comparator-function="exact" match-threshold="0.8" weight="1" desc="门诊号" />

</match-fields>
