<?xml version="1.0" encoding="UTF-8"?>
<basic-blocking xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="blocking.xsd">

	<!-- 业务初筛默认配置 -->
	<blocking-rounds>

		<!-- 住院号 -->
		<blocking-round>
			<blocking-fields>
				<blocking-field db-field="BIZ_INPATIENTNO">
					<field-name>BIZ_INPATIENTNO</field-name>
				</blocking-field>
			</blocking-fields>
		</blocking-round>

		<!-- 门诊号 -->
		<blocking-round>
			<blocking-fields>
				<blocking-field db-field="BIZ_CLINICNO">
					<field-name>BIZ_CLINICNO</field-name>
				</blocking-field>
			</blocking-fields>
		</blocking-round>

	</blocking-rounds>
</basic-blocking>

